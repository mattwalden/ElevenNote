@{
    var fizzBuzzList = new List<string>();

    for (int i = 1; i <= 100; i++)
    {
        if (i % 3 == 0 && i % 5 == 0) { fizzBuzzList.Add("FizzBuzz"); }
        else if (i % 3 == 0)
        {
            fizzBuzzList.Add("Fizz");
        }
        else if (i % 5 == 0)
        {

            fizzBuzzList.Add("Buzz");
        }
        else
        {
            fizzBuzzList.Add(i.ToString());
        }
    }

    {


    }

    <h1>Walden</h1>
     <select name="theOption" onchange=" ">
        @foreach (var s in fizzBuzzList)
        {
            <option value="@s">@s</option>
        }
    </select>
}
